{
  "name": "portfolio",
  "homepage": "https://arkadyt.com/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-icons": "^3.3.0",
    "react-scripts": "^2.1.3"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.5",
    "react-app-rewire-preact": "^1.0.1",
    "react-app-rewired": "^2.0.2-next.0"
  },
  "scripts": {
    "start-js": "react-app-rewired start",
    "build-js": "react-app-rewired build",
    "build-css": "node-sass-chokidar --include-path ./src src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-app-rewired test --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "firebase deploy",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
